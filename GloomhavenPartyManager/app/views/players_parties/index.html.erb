<p id="notice"><%= notice %></p>

<h1>Players Parties</h1>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>Party</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players_parties.each do |players_party| %>
      <tr>
        <td><%= players_party.player_id %></td>
        <td><%= players_party.party_id %></td>
        <td><%= link_to 'Show', players_party %></td>
        <td><%= link_to 'Edit', edit_players_party_path(players_party) %></td>
        <td><%= link_to 'Destroy', players_party, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Players Party', new_players_party_path %>
